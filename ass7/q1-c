#include<iostream>
using namespace std;

int main(){
    int n,a[100];
    cout<<"Enter size: ";
    cin>>n;
    cout<<"Enter elements:\n";
    for(int i=0;i<n;i++)
    cin>>a[i];
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-i-1;j++){
            if(a[j]>a[j+1]){
                int t=a[j];
                a[j]=a[j+1];
                a[j+1]=t;
            }
        }
    }
    cout<<"Bubble Sorted: ";
    for(int i=0;i<n;i++)
    cout<<a[i]<<" ";
}
